{
  "name": "react-electron-mpa",
  "version": "0.0.0",
  "private": true,
  "author": "northwang-lucky",
  "description": "React electron MPA application",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "sv": "standard-version",
    "prepare": "rimraf ./.husky && husky install && pnpm husky:pre-commit-hook && pnpm husky:commit-msg-hook",
    "commit": "cz",
    "commit:lint": "commitlint --edit",
    "husky:pre-commit-hook": "husky add .husky/pre-commit \"pnpm lint-staged\"",
    "husky:commit-msg-hook": "husky add .husky/commit-msg \"pnpm commit:lint\"",
    "lint-staged": "lint-staged",
    "lint-staged:format": "prettier -w",
    "lint-staged:eslint": "eslint"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "babel-loader": "9.1.2",
    "commitizen": "4.3.0",
    "css-loader": "6.7.3",
    "cssnano": "6.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "html-webpack-plugin": "5.5.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "2.7.5",
    "postcss": "8.4.23",
    "postcss-load-config": "4.0.1",
    "postcss-loader": "7.3.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resolve-url-loader": "5.0.0",
    "rimraf": "4.0.7",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "standard-version": "9.5.0",
    "typescript": "4.9.4",
    "webpack": "5.82.0",
    "webpack-cli": "5.1.0",
    "webpack-dev-server": "4.15.0",
    "webpack-merge": "5.8.0"
  }
}
